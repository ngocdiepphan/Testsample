\n.icon-next-right-mb {\n  background-image: url('../../assets/images/icons/next-right.svg');\n  width: 62px;\n  height: 130px;\n}\n/* .feedback-info {\n\n} */\n\n.feedback-rating {\n  display: flex;\n  flex-direction: row;\n}\n\n.feedback-description {\n  font-weight: 500;\n  font-size: 18px;\n  line-height: 2.25;\n  color: var(--color-primary);\n}\n\n.feedback-profile {\n  display: flex;\n  flex-direction: column;\n  gap: 10px;\n}\n\n.feedback-username {\n  font-size: 20px;\n  font-weight: 500;\n}\n\n.feature-position {\n  font-size: 14px;\n  color: #6d7781;\n}\n\n.facebook-slider {\n  display: flex;\n  flex-direction: row;\n  gap: 70px;\n  margin-top: 30px;\n}\n@media (min-width: 1200px) {\n\n  .feedback-wrapper {\n    display: grid;\n    grid-template-columns: repeat(4, 1fr);\n    align-items: center;\n    justify-items: center;\n    gap: 18px;\n    height: 400px;\n  }\n\n  .feedback-image-below {\n    width: 330px;\n    height: 400px;\n  }\n\n  .feedback-content {\n    width: 360px;\n  }\n\n  .feedback-description {\n    font-size: 20px;\n    line-height: 2.25;\n  }\n\n  .feedback-username {\n    font-size: 24px;\n    font-weight: 500;\n  }\n\n  .feature-position {\n    font-size: 16px;\n  }\n\n  .icon-next-left {\n    background-image: url('../../assets/images/icons/next-left.svg');\n    width: 60px;\n    height: 74px;\n    display: block;\n  }\n\n  .icon-next-right {\n    background-image: url('../../assets/images/icons/next-right.svg');\n    width: 62px;\n    height: 130px;\n    display: block;\n  }\n\n  .icon-next-left-mb {\n    display: none;\n  }\n\n  .icon-next-right-mb {\n    display: none;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"a7132a6c576a5204b5596469fadfa1e8","cacheData":{"env":{}}}